[user]
	email = 38206553+rajyan@users.noreply.github.com
	name = Yohta Kimura
[alias]
	comend = commit --amend --no-edit
	logg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	dif = diff --name-only
	difff = diff --word-diff
[rerere]
	enabled = true
[color]
	ui = auto
[color "branch"]
	current = green bold
	local = cyan
	remote = red
[color "status"]
	added = green
	changed = yellow
	untracked = red
[pull]
	rebase = false
[core]
	autocrlf = input
[credential]
	helper = manager
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
